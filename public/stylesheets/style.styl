body
  font: 14px "Helvetica Neue", Helvetica, Arial, sans-serif
  margin 0
  padding 0
  border 0

#header
  color white
  background orangered

#head
  font-style italic

.headList li
  padding-right 20px
  display: inline-block;
  list-style-type: none;

.headList a
  color black
  border-width 5px

a
  color: #00B7FF

h2, h3
  margin: 5px auto

.prize, .proof
  margin: 5px auto

.proof
  text-align center

.prize h2, .proof h3
  display: inline

.vote
  cursor pointer

.up
  color green

.down
  color red

.masonry {
  background: #EEE;
  max-width: 1000px;
  margin-left auto
  margin-right auto
}

.masonry .item {
  width:  300px;
  float: left;
  margin 5px;
  padding 5px
  background: #888;
  opacity 0.7
  border-radius: 5px;
}
